<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<view name="system">
		<image name="logo" ifSubset="systemlogocops1:logosimple">
			<linearSmooth>true</linearSmooth>
			<path>${bobmediapath}/_systemmedia/_logosyst/clearlogos/${system.theme}.png</path>
			<path>${bobmediapath}/_systemmedia/_logosyst/clearlogos/region-${region}/${system.theme}-${region}.png</path>
			<path>${bobmediapath}/_systemmedia/_logosyst/clearlogos/lang-${lang}/${system.theme}-${lang}.png</path>
		</image>
		<image name="logo" ifSubset="systemlogocops1:logobd|logoovalcarb|logoovalcarbfresh|hmaxovalsilver|logorect|logorectfresh|hmaxrectrefl|glassnekhros">
			<linearSmooth>true</linearSmooth>
			<path ifSubset="systemlogocops1:logobd">${extlogobd}</path>
			<path ifSubset="systemlogocops1:logoovalcarb">${extlogohcarbon}</path>
			<path ifSubset="systemlogocops1:hmaxovalsilver">${exthmaxovalsilver}</path>
			<path ifSubset="systemlogocops1:logorect">${extlogohrect}</path>
			<path ifSubset="systemlogocops1:logoovalcarbfresh">${extlogohcarbonfresh}</path>
			<path ifSubset="systemlogocops1:logorectfresh">${extlogohrectfresh}</path>
			<path ifSubset="systemlogocops1:hmaxrectrefl">${exthmaxrectrefl}</path>
			<path ifSubset="systemlogocops1:glassnekhros">${extglassnekhros}</path>
		</image>
	</view>
</theme>