<storyboard event="open">
<animation property="opacity" from="1" to="1" duration="400" mode="linear"/>
<animation property="offsetX" to="0.15" duration="400" mode="easeIn" begin="1000"/>
</storyboard>
<storyboard event="activateNext">
<animation property="opacity" from="1" to="1" duration="1" mode="linear"/>
<animation property="offsetX" to="0.15" duration="600" mode="easeIn" begin="1000"/>
</storyboard>
<storyboard event="activatePrev">
<animation property="opacity" from="1" to="1" duration="1" mode="linear"/>
<animation property="offsetX" to="0.15" duration="600" mode="easeIn" begin="1000"/>
</storyboard>
<storyboard event="deactivateNext">
<animation property="opacity" to="0" duration="1" mode="easeOut"/>
</storyboard>
<storyboard event="deactivatePrev">
<animation property="opacity" to="0" duration="1" mode="easeOut"/>
</storyboard>
</control>

	
