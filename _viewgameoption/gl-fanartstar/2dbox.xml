<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<customView name="presetstarbox" displayName="${presetstarbox}" inherits="gamecarousel">
		<gamecarousel name="gamecarousel">
			<maxLogoCount ifSubset="spacebox:spacemore">9</maxLogoCount>
			<maxLogoCount ifSubset="spacebox:spaceplus">10</maxLogoCount>
			<maxLogoCount ifSubset="spacebox:spacesmall">12</maxLogoCount>
			<maxLogoCount ifSubset="spacebox:spacevery">17</maxLogoCount>
			<maxLogoCount ifSubset="spacebox:spacenorm">11</maxLogoCount>
			<scaledLogoSpacing ifSubset="spacebox:spacevery">3</scaledLogoSpacing>
			<scaledLogoSpacing ifSubset="spacebox:spacesmall">1</scaledLogoSpacing>
			<scaledLogoSpacing ifSubset="spacebox:spacenorm">0.2</scaledLogoSpacing>
		</gamecarousel>
	</customView>
</theme>
