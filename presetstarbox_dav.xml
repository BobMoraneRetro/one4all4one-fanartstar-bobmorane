<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<customView name="presetstarbox" displayName="${presetstarbox}" inherits="gamecarousel">
		<control name="fanart_extra,screenshot_extra,video_extra">
			<visible>false</visible>
		</control>
		<image name="md_image">
			<visible>false</visible>
		</image>
		<gamecarousel name="gamecarousel">
			<zIndex>10</zIndex>
			<type>horizontal</type>
			<pos>-0.1 0.1</pos>
			<pos ifHelpPrompts="true">-0.1 0.08</pos>
			<size>1.45 1</size>
			<logoScale>1.5</logoScale>
			<logoSize>0.350 0.350</logoSize>
			<color>00000000</color>
			<logoAlignment>bottom</logoAlignment>
			<minLogoOpacity>1</minLogoOpacity>
			<transitionSpeed>1050</transitionSpeed>
			<logoPos>0.15 0.5</logoPos>
			<imageSource>thumbnail</imageSource>
			<defaultTransition>instant</defaultTransition>
			<itemTemplate>
				<container>
					<image name="thumbnail">
						<visible if="{system.theme} == 'snes' || {system.theme} == 'n64' || {system.theme} == 'gameandwatch' || {system.theme} == 'solarus' || {system.theme} == 'ngage'">false</visible>
						<pos>0.5</pos>
						<pos>0.5 0.6</pos>
						<origin>0.5</origin>
						<maxSize>0.38 1</maxSize>
						<path>{game:thumbnail}</path>
						<path ifSubset="starthumb:starbox3d">firstfile(${core3dart},{game:boxart})</path>
						<path ifSubset="starthumb:starbox2d">firstfile(${corefrontart},{game:thumbnail})</path>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.65" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
						<shader>
							<path>:/shaders/border.glsl</path>
							<cornerRadius>0.03</cornerRadius>
							<borderSize>0</borderSize>
							<borderColor>00000080</borderColor>
							<innerShadowSize>0.02</innerShadowSize>
							<innerShadowColor>00000070</innerShadowColor>
							<outerShadowSize>0.02</outerShadowSize>
							<outerShadowColor>00000080</outerShadowColor>
						</shader>
					</image>
					<image name="thumbnail">
						<visible>false</visible>
						<visible if="{system.theme} == 'snes' || {system.theme} == 'n64' || {system.theme} == 'gameandwatch'|| {system.theme} == 'solarus' || {system.theme} == 'ngage'">{game:region} != 'jp'</visible>
						<pos>0.5 0.6</pos>
						<origin>0.5</origin>
						<rotation>90</rotation>
						<maxSize>0.55 1</maxSize>
						<path>{game:thumbnail}</path>
						<path ifSubset="starthumb:starbox3d">firstfile(${core3dart},{game:boxart})</path>
						<path ifSubset="starthumb:starbox2d">firstfile(${corefrontart},{game:thumbnail})</path>
						<storyboard event="open">
							<animation property="rotation" to="87" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-5" begin="250" duration="250" mode="easeOut"/>
							<animation property="y" to="0.68" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="rotation" to="90" begin="" duration="150" mode="Linear"/>
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
						<shader>
							<path>:/shaders/border.glsl</path>
							<cornerRadius>0.03</cornerRadius>
							<borderSize>0</borderSize>
							<borderColor>00000080</borderColor>
							<innerShadowSize>0.02</innerShadowSize>
							<innerShadowColor>00000070</innerShadowColor>
							<outerShadowSize>0.02</outerShadowSize>
							<outerShadowColor>00000080</outerShadowColor>
						</shader>
					</image>
					<image name="thumbnail">
						<visible>false</visible>
						<visible if="{system.theme} == 'snes' || {system.theme} == 'n64' || {system.theme} == 'gameandwatch'|| {system.theme} == 'solarus' || {system.theme} == 'ngage'">{game:region} == 'jp'</visible>
						<pos>0.5 0.6</pos>
						<origin>0.5</origin>
						<rotation>0</rotation>
						<maxSize>0.38 1</maxSize>
						<path>{game:thumbnail}</path>
						<path ifSubset="starthumb:starbox3d">firstfile(${core3dart},{game:boxart})</path>
						<path ifSubset="starthumb:starbox2d">firstfile(${corefrontart},{game:thumbnail})</path>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.65" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
						<shader>
							<path>:/shaders/border.glsl</path>
							<cornerRadius>0.03</cornerRadius>
							<borderSize>0</borderSize>
							<borderColor>00000080</borderColor>
							<innerShadowSize>0.02</innerShadowSize>
							<innerShadowColor>00000070</innerShadowColor>
							<outerShadowSize>0.02</outerShadowSize>
							<outerShadowColor>00000080</outerShadowColor>
						</shader>
					</image>
					<image name="nothumb">
						<visible>!exists({game:thumbnail}) &amp;&amp; !exists(${corefrontart})</visible>
						<pos>0.5 0.6</pos>
						<origin>0.5</origin>
						<rotation>0</rotation>
						<minSize>0.38 0.96</minSize>
						<minSize if="{system.theme} == 'dreamcast' 
						|| {system.theme} == '3do' 
						|| {system.theme} == 'amigacdtv' 
						|| {system.theme} == 'amigacd32' 
						|| {system.theme} == 'gx4000' 
						|| {system.theme} == 'cdi' 
						|| {system.theme} == 'neogeocd' 
						|| {system.theme} == 'pce-cd' 
						|| {system.theme} == 'pcfx' 
						|| {system.theme} == 'psx' 
						|| {system.theme} == 'saturn' 
						|| {system.theme} == 'segacd'">0.38 0.69</minSize>
						<path>${intfanartfresh}</path>
						<saturation>0.7</saturation>
						<shader>
							<path>:/shaders/border.glsl</path>
							<cornerRadius>0.03</cornerRadius>
							<borderSize>0</borderSize>
							<borderColor>00000080</borderColor>
							<innerShadowSize>0.02</innerShadowSize>
							<innerShadowColor>00000070</innerShadowColor>
							<outerShadowSize>0.02</outerShadowSize>
							<outerShadowColor>00000080</outerShadowColor>
						</shader>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.65" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
					</image>
					<image name="nothumbmarq">
						<visible>!exists({game:thumbnail}) &amp;&amp; !exists(${corefrontart})</visible>
						<pos>0.5 0.55</pos>
						<origin>0.5</origin>
						<rotation>0</rotation>
						<maxSize>0.33 1</maxSize>
						<path>{game:marquee}</path>
						<path if="{system.theme} == 'switch'">${extgameartpng}</path>
						<shader>
							<path>:/shaders/border.glsl</path>
							<cornerRadius>0.03</cornerRadius>
							<borderSize>0</borderSize>
							<borderColor>00000080</borderColor>
							<innerShadowSize>0.02</innerShadowSize>
							<innerShadowColor>00000070</innerShadowColor>
							<outerShadowSize>0.02</outerShadowSize>
							<outerShadowColor>00000080</outerShadowColor>
						</shader>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.65" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
					</image>
					<text name="gamename" if="{system.theme} == 'switch'">
						<visible>!exists({game:thumbnail}) &amp;&amp; !exists(${corefrontart})</visible>
						<origin>0.5</origin>
						<text>{game:name}</text>
						<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
						<color>FFFFFF</color>
						<alignment>center</alignment>
						<fontSize>0.025</fontSize>
						<lineSpacing>1</lineSpacing>
						<size>0.3 2</size>
						<pos>0.5 0.9</pos>
						<glowColor>00000066</glowColor>
						<glowSize>2</glowSize>
						<zIndex>400</zIndex>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.87" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="y" to="0.85" begin="" duration="" mode="Linear"/>
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.85" begin="" duration="" mode="Linear"/>
						</storyboard>
					</text>
					<text name="gamename" if="{system.theme} != 'switch'">
						<visible>!exists({game:marquee}) &amp;&amp; !exists({game:thumbnail}) &amp;&amp; !exists(${corefrontart})</visible>
						<origin>0.5</origin>
						<text>{game:name}</text>
						<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
						<color>FFFFFF</color>
						<alignment>center</alignment>
						<fontSize>0.025</fontSize>
						<lineSpacing>1</lineSpacing>
						<size>0.3 2</size>
						<pos>0.5 0.8</pos>
						<glowColor>00000066</glowColor>
						<glowSize>2</glowSize>
						<zIndex>400</zIndex>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
							<animation property="y" to="0.65" begin="" duration="350" mode="bump"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
							<animation property="rotation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="y" to="0.6" begin="" duration="" mode="Linear"/>
						</storyboard>
					</text>
				</container>
			</itemTemplate>
		</gamecarousel>
		<image name="gamecarouselLogo">
			<linearSmooth>true</linearSmooth>
		</image>
		<!-- ####################### Gestion des fanarts et Overlays ########################### -->
		<image name="defautfanart" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttest}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttest}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttest}</visible>
			<path>${intfanartfresh}</path>
			<autoFade>false</autoFade>
			<pos>0.5 0.5</pos>
			<size>0 1</size>
			<opacity>0.5</opacity>
			<saturation>1</saturation>
			<origin>0.5 0.5</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>2</zIndex>
		</image>
		<image name="fanartblur" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path ifSubset="custombuilds:standardbuilds,customfanart:fanartcustom">firstfile(${corefanart},${standardsbuildsfanart},${freshbuildfanart})</path>
			<path ifSubset="custombuilds:retrofreshbat,customfanart:fanartcustom">firstfile(${corefanart},${freshbuildfanart},${standardsbuildsfanart})</path>
			<path ifSubset="custombuilds:standardbuilds,customfanart:fanartgamelist">${standardsbuildsfanart}</path>
			<path ifSubset="custombuilds:retrofreshbat,customfanart:fanartgamelist">${freshbuildfanart}</path>
			<autoFade>false</autoFade>
			<pos>0.5 0.5</pos>
			<size>0 1</size>
			<origin>0.5 0.5</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>1</zIndex>
		</image>
		<screenshader name="blurback" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>1 1</size>
			<pos>0 0</pos>
			<blur>10</blur>
			<zIndex>1</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<screenshader name="blurleft1" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>0.11 1</size>
			<pos>0 0</pos>
			<blur>8</blur>
			<zIndex>3</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<screenshader name="blurleft" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>0.12 1</size>
			<pos>0 0</pos>
			<blur>6</blur>
			<zIndex>3</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<!-- #################### Fanart principal ############################ -->
		<image name="fanart" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path ifSubset="custombuilds:standardbuilds,customfanart:fanartcustom">firstfile(${corefanart},${standardsbuildsfanart},${freshbuildfanart})</path>
			<path ifSubset="custombuilds:retrofreshbat,customfanart:fanartcustom">firstfile(${corefanart},${freshbuildfanart},${standardsbuildsfanart})</path>
			<path ifSubset="custombuilds:standardbuilds,customfanart:fanartgamelist">${standardsbuildsfanart}</path>
			<path ifSubset="custombuilds:retrofreshbat,customfanart:fanartgamelist">${freshbuildfanart}</path>
			<!-- <path ifSubset="customfanart:fanartcustom">${corefanart}</path> -->
			<!-- <path ifSubset="custombuilds:standardbuilds">${standardsbuildsfanart}</path> -->
			<!-- <path ifSubset="custombuilds:retrofreshbat">${freshbuildfanart}</path> -->
			<autoFade>false</autoFade>
			<pos>0.5 0</pos>
			<size>0 0.8</size>
			<origin>0.5 0</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>2</zIndex>
		</image>
		<!-- #################### Fin Fanart principal ############################ -->
		<screenshader name="blurright2" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>0.15 1</size>
			<pos>0.855 0</pos>
			<blur>10</blur>
			<zIndex>3</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<screenshader name="blurright1" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>0.18 1</size>
			<pos>0.853 0</pos>
			<blur>8</blur>
			<zIndex>4</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<screenshader name="blurright" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>0.2 1</size>
			<pos>0.85 0</pos>
			<blur>6</blur>
			<zIndex>5</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<screenshader name="blurbottom2" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<visible ifSubset="custombuilds:coretyperfanart">${corefanarttestyes}</visible>
			<path>:/shaders/blur.glsl</path>
			<size>1 0.3</size>
			<pos>0 0.695</pos>
			<blur>10</blur>
			<zIndex>3</zIndex>
			<blurColor>000000</blurColor>
		</screenshader>
		<!-- ############################# Overlays ############################################################################################################# -->
		<image name="overlaybase" extra="true">
			<path>${themePath}/_viewgame/fanartstar/overlay.png</path>
			<size>1.1 1.1</size>
			<flipX>false</flipX>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<zIndex>30</zIndex>
		</image>
		<image name="overlaydiamonds" extra="true">
			<path>${themePath}/_viewgame/fanartstar/overlay.png</path>
			<size>1 1</size>
			<flipX>true</flipX>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<zIndex>2</zIndex>
		</image>
		<image name="fadebottom" extra="true">
			<path>${themePath}/_viewgame/fanartstar/overlaydown.png</path>
			<size>1 1</size>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<zIndex>3</zIndex>
		</image>
		<!-- ############################# Graphismes et Metadata ################################ -->
		<!-- Logo systeme en fonction de la region du jeu -->
		<image name="logosytemgamelistall" extra="true">
			<visible>{game:region} != 'us' &amp;&amp; {game:region} != 'eur' &amp;&amp; {game:region} != 'fr' &amp;&amp; {game:region} != 'jp'</visible>
			<origin>0.5 0.5</origin>
			<pos>0.94 0.045</pos>
			<path>${intlogoclearpng}</path>
			<maxSize>0.1 0.1</maxSize>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<zIndex>80</zIndex>
		</image>
		<image name="logosytemgamelistus" extra="true">
			<visible>{game:region} == 'us'</visible>
			<origin>0.5 0.5</origin>
			<pos>0.94 0.045</pos>
			<path>${intlogoclearpng}</path>
			<path>${intlogoclearuspng}</path>
			<maxSize>0.1 0.1</maxSize>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<zIndex>80</zIndex>
		</image>
		<image name="logosytemgamelistfr" extra="true">
			<visible>{game:region} == 'fr'</visible>
			<origin>0.5 0.5</origin>
			<pos>0.94 0.045</pos>
			<path>${intlogoclearpng}</path>
			<path>${intlogoclearfrpng}</path>
			<maxSize>0.1 0.1</maxSize>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<zIndex>80</zIndex>
		</image>
		<image name="logosytemgamelisteur" extra="true">
			<visible>{game:region} == 'eu'</visible>
			<origin>0.5 0.5</origin>
			<pos>0.94 0.045</pos>
			<path>${intlogoclearpng}</path>
			<path>${intlogoclearfrpng}</path>
			<maxSize>0.1 0.1</maxSize>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<zIndex>80</zIndex>
		</image>
		<image name="logosytemgamelist" extra="true">
			<visible>{game:region} == 'jp'</visible>
			<origin>0.5 0.5</origin>
			<pos>0.94 0.045</pos>
			<path>${intlogoclearpng}</path>
			<path>${intlogoclearjppng}</path>
