<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<customView name="presetstar43" displayName="${presetstar43}" inherits="grid">
		<control name="fanart_extra, defautfanartextra">
			<visible>false</visible>
		</control>
		<video name="md_video">
			<visible>false</visible>
		</video>
		<imagegrid name="gamegrid">
			<pos>-0.0302 0.72</pos>
			<pos ifHelpPrompts="true">-0.0302 0.69</pos>
			<size>1.06 0.35</size>
			<autoLayout>7 1</autoLayout>
			<animateSelection>false</animateSelection>
			<zIndex>50</zIndex>
			<autoLayoutSelectedZoom>1.5</autoLayoutSelectedZoom>
			<imageSource ifSubset="custombuilds:standardbuilds">fanart</imageSource>
			<imageSource ifSubset="custombuilds:retrofreshbat">image</imageSource>
			<padding>0.03 0.05 0.03 0.12</padding>
			<margin>0 0.03</margin>
			<showVideoAtDelay>100</showVideoAtDelay>
			<centerSelection>true</centerSelection>
			<scrollLoop>true</scrollLoop>
			<scrollDirection>horizontal</scrollDirection>
		</imagegrid>
		<gridtile name="default">
			<size>0.5 0.35</size>
			<padding>2 2</padding>
			<backgroundCenterColor>FFFFFF00</backgroundCenterColor>
			<backgroundEdgeColor>909090</backgroundEdgeColor>
			<!-- Bleu -->
			<backgroundEdgeColor ifSubset="gridcolor:skyblue">0099ff</backgroundEdgeColor>
			<!-- Bleu Dark -->
			<backgroundEdgeColor ifSubset="gridcolor:navy">000080</backgroundEdgeColor>
			<!-- Candy -->
			<backgroundEdgeColor ifSubset="gridcolor:candy">FF0040</backgroundEdgeColor>
			<!-- Vert -->
			<backgroundEdgeColor ifSubset="gridcolor:green">00ff33</backgroundEdgeColor>
			<!-- Vert alt -->
			<backgroundEdgeColor ifSubset="gridcolor:greenalt">2baf2b</backgroundEdgeColor>
			<!-- Gris -->
			<backgroundEdgeColor ifSubset="gridcolor:grey">333333</backgroundEdgeColor>
			<!-- Gris -->
			<backgroundEdgeColor ifSubset="gridcolor:lightgrey">D3D3D3</backgroundEdgeColor>
			<!-- Orange -->
			<backgroundEdgeColor ifSubset="gridcolor:orange">ff6600</backgroundEdgeColor>
			<!-- Rose -->
			<backgroundEdgeColor ifSubset="gridcolor:pink">ff0099</backgroundEdgeColor>
			<!-- Plus -->
			<backgroundEdgeColor ifSubset="gridcolor:standard">00c6ff</backgroundEdgeColor>
			<!-- Purple Alt -->
			<backgroundEdgeColor ifSubset="gridcolor:purplealt">554488</backgroundEdgeColor>
			<!-- Purple -->
			<backgroundEdgeColor ifSubset="gridcolor:violet">330099</backgroundEdgeColor>
			<!-- Red-alt -->
			<backgroundEdgeColor ifSubset="gridcolor:redalt">7F1425</backgroundEdgeColor>
			<!-- Red -->
			<backgroundEdgeColor ifSubset="gridcolor:red">FF0000</backgroundEdgeColor>
			<!-- Violet -->
			<backgroundEdgeColor ifSubset="gridcolor:purple">c377e0</backgroundEdgeColor>
			<!-- Yellow-alt -->
			<backgroundEdgeColor ifSubset="gridcolor:yellowalt">909090</backgroundEdgeColor>
			<!-- Yellow -->
			<backgroundEdgeColor ifSubset="gridcolor:yellow">ffff00</backgroundEdgeColor>
			<!-- Blanc -->
			<backgroundEdgeColor ifSubset="gridcolor:default">ffffff</backgroundEdgeColor>
			<backgroundCornerSize>0 0</backgroundCornerSize>
			<selectionMode>image</selectionMode>
			<imageSizeMode>minSize</imageSizeMode>
			<imageColor>858585ff</imageColor>
		</gridtile>
		<gridtile name="selected">
			<padding>6 6</padding>
			<!-- Bleu -->
			<backgroundEdgeColor ifSubset="gridcolor:skyblue">0099ff</backgroundEdgeColor>
			<!-- Bleu Dark -->
			<backgroundEdgeColor ifSubset="gridcolor:navy">000080</backgroundEdgeColor>
			<!-- Candy -->
			<backgroundEdgeColor ifSubset="gridcolor:candy">FF0040</backgroundEdgeColor>
			<!-- Vert -->
			<backgroundEdgeColor ifSubset="gridcolor:green">00ff33</backgroundEdgeColor>
			<!-- Vert alt -->
			<backgroundEdgeColor ifSubset="gridcolor:greenalt">2baf2b</backgroundEdgeColor>
			<!-- Gris -->
			<backgroundEdgeColor ifSubset="gridcolor:grey">333333</backgroundEdgeColor>
			<!-- Gris -->
			<backgroundEdgeColor ifSubset="gridcolor:lightgrey">D3D3D3</backgroundEdgeColor>
			<!-- Orange -->
			<backgroundEdgeColor ifSubset="gridcolor:orange">ff6600</backgroundEdgeColor>
			<!-- Rose -->
			<backgroundEdgeColor ifSubset="gridcolor:pink">ff0099</backgroundEdgeColor>
			<!-- Plus -->
			<backgroundEdgeColor ifSubset="gridcolor:standard">00c6ff</backgroundEdgeColor>
			<!-- Purple Alt -->
			<backgroundEdgeColor ifSubset="gridcolor:purplealt">554488</backgroundEdgeColor>
			<!-- Purple -->
			<backgroundEdgeColor ifSubset="gridcolor:violet">330099</backgroundEdgeColor>
			<!-- Red-alt -->
			<backgroundEdgeColor ifSubset="gridcolor:redalt">7F1425</backgroundEdgeColor>
			<!-- Red -->
			<backgroundEdgeColor ifSubset="gridcolor:red">FF0000</backgroundEdgeColor>
			<!-- Violet -->
			<backgroundEdgeColor ifSubset="gridcolor:purple">c377e0</backgroundEdgeColor>
			<!-- Yellow-alt -->
			<backgroundEdgeColor ifSubset="gridcolor:yellowalt">909090</backgroundEdgeColor>
			<!-- Yellow -->
			<backgroundEdgeColor ifSubset="gridcolor:yellow">ffff00</backgroundEdgeColor>
			<!-- Blanc -->
			<backgroundEdgeColor ifSubset="gridcolor:default">ffffff</backgroundEdgeColor>
			<backgroundCornerSize>0</backgroundCornerSize>
			<imageColor>FFFFFFFF</imageColor>
		</gridtile>
		<image name="gridtile.marquee">
			<color>A5A5A5ff</color>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<size>0.7 0.5</size>
		</image>
		<image name="gridtile.marquee:selected">
			<pos>0.5 0.5</pos>
			<color>FFFFFFFF</color>
			<size>0.75 0.75</size>
		</image>
		<image name="gridtile.favorite:selected">
			<color>ffff00</color>
		</image>
		<image name="gridtile.marquee:videoplaying">
			<color>FFFFFFA0</color>
			<!-- <pos>0.5 0</pos> -->
			<size>0 0</size>
		</image>
		<image name="gridtile.favorite">
			<pos>0.9 0.10</pos>
			<maxSize>0.2 0.2</maxSize>
			<origin>0.5 0.5</origin>
			<horizontalAlignment>right</horizontalAlignment>
		</image>
		<!-- cadre clignotant -->
		<ninepatch name="gridtile.background:selected">
			<cornerSize>0 0</cornerSize>
			<padding>-3 -3</padding>
			<color>ffffff</color>
			<animateColor>141414</animateColor>
			<animateColorTime>850</animateColorTime>
			<zIndex>500</zIndex>
		</ninepatch>
		<image name="defautfanart" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttest}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttest}</visible>
			<path>${intfanartfresh}</path>
			<autoFade>false</autoFade>
			<pos>0.5 0.5</pos>
			<size>0 1</size>
			<opacity>0.5</opacity>
			<saturation>0</saturation>
			<origin>0.5 0.5</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>2</zIndex>
		</image>
		<image name="defautboxart" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttest}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttest}</visible>
			<path>{game:thumbnail}</path>
			<autoFade>false</autoFade>
			<pos>0.96 0.7</pos>
			<maxSize>0.4 0.6</maxSize>
			<origin>1 1</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>2</zIndex>
		</image>
		<image name="fanart" extra="true">
			<visible ifSubset="custombuilds:standardbuilds">${standardsbuildsfanarttestyes}</visible>
			<visible ifSubset="custombuilds:retrofreshbat">${freshbuildfanarttestyes}</visible>
			<path ifSubset="custombuilds:standardbuilds">${standardsbuildsfanart}</path>
			<path ifSubset="custombuilds:retrofreshbat">${freshbuildfanart}</path>
			<autoFade>false</autoFade>
			<pos>0.5 0.5</pos>
			<size>0 1</size>
			<origin>0.5 0.5</origin>
			<linearSmooth>true</linearSmooth>
			<zIndex>2</zIndex>
		</image>
		<image name="overlaybase" extra="true">
			<path>${themePath}/_viewgame/fanartstar/overlaydiamonds.png</path>
			<size>1 1</size>
			<flipX>true</flipX>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<zIndex>3</zIndex>
		</image>
		<image name="fadebottom" extra="true">
			<path>${themePath}/_viewgame/fanartstar/overlaydown.png</path>
			<size>1 1</size>
			<pos>0.5 0.5</pos>
			<origin>0.5 0.5</origin>
			<zIndex>3</zIndex>
		</image>
		<!-- MetaDATA -->
		<image name="logosytemgamelist" extra="true">
			<origin>0.5 0.5</origin>
			<pos>0.94 0.04</pos>
			<path>${intlogoclearpng}</path>
			<maxSize>0.1 0.1</maxSize>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<zIndex>80</zIndex>
		</image>
		<image name="md_wheel">
			<pos>0.05 0.165</pos>
			<origin>0 0.5</origin>
			<maxSize>0.25 0.23</maxSize>
			<zIndex>10</zIndex>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
		</image>
		<text name="gamename" extra="true">
			<text>{game:name}</text>
			<opacity>0.7</opacity>
			<pos>0.252 0.63</pos>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<fontSize>0.025</fontSize>
			<color>ffffffff</color>
			<origin>0.5 0.5</origin>
			<size>0.4 0.02</size>
			<alignment>left</alignment>
			<glowColor>00000016</glowColor>
			<glowSize>3</glowSize>
			<zIndex>305</zIndex>
		</text>
		<text name="developer" extra="true">
			<color>EFEFFF</color>
			<pos>0.0525 0.630</pos>
			<size>0.7 0.05</size>
			<text>${presgame1} {game:genre} ${presgame2} {game:developer} ${presgame3} {game:system} ${presgame4} {game:lang}</text>
			<alignment>left</alignment>
			<fontSize>0.015</fontSize>
		</text>
		<text name="md_description">
			<color>EFEFFF</color>
			<pos>0.0525 0.666</pos>
			<size>0.3 0.035</size>
			<!-- <text>{game:folder}</text> -->
			<alignment>left</alignment>
			<fontSize>0.015</fontSize>
			<singleLineScroll>false</singleLineScroll>
			<lineSpacing>1</lineSpacing>
		</text>
		<!-- Meta dans rectangle noir -->
		<image name="playblack" extra="true">
			<tile>false</tile>
			<origin>0.5 0.5</origin>
			<pos>0.085 0.72</pos>
			<maxSize>0.085 0.085</maxSize>
			<zIndex>305</zIndex>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<path>${themePath}/_viewgame/fanartstar/Bvierge.png</path>
		</image>
		<text name="binds_playcount" extra="true">
			<visible>{game:playcount} &gt; 0 ? 1 : 0</visible>
			<color>FFFFFF</color>
			<text>{game:playcount} ${playcount}</text>
			<origin>0.5 0.5</origin>
			<pos>0.085 0.7185</pos>
			<size>0.08 0.035</size>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<text name="binds_noplaycount" extra="true">
			<visible>{game:playcount} &gt; 0 ? 0 : 1</visible>
			<color>FFFFFF</color>
			<text>${noplaycount}</text>
			<origin>0.5 0.5</origin>
			<pos>0.085 0.7185</pos>
			<size>0.08 0.035</size>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<image name="dateblack" extra="true">
			<tile>false</tile>
			<origin>0.5 0.5</origin>
			<pos>0.16 0.72</pos>
			<maxSize>0.085 0.085</maxSize>
			<zIndex>300</zIndex>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<path>${themePath}/_viewgame/fanartstar/Bvierge.png</path>
		</image>
		<text name="binds_releasedate" extra="true">
			<visible>{game:releaseyear} &gt; 0 ? 1 : 0</visible>
			<color>FFFFFF</color>
			<text>{game:releaseyear}</text>
			<origin>0.5 0.5</origin>
			<pos>0.16 0.7185</pos>
			<size>0.05 0.035</size>
			<forceUppercase>0</forceUppercase>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<text name="binds_noreleasedate" extra="true">
			<visible>{game:releaseyear} &gt; 0 ? 0 : 1</visible>
			<color>FFFFFF</color>
			<text>${nodates}</text>
			<origin>0.5 0.5</origin>
			<pos>0.16 0.7185</pos>
			<size>0.1 0.035</size>
			<forceUppercase>0</forceUppercase>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<image name="playerblack" extra="true">
			<tile>false</tile>
			<origin>0.5 0.5</origin>
			<pos>0.235 0.72</pos>
			<maxSize>0.085 0.085</maxSize>
			<zIndex>205</zIndex>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<path>${themePath}/_viewgame/fanartstar/Bvierge.png</path>
		</image>
		<text name="binds_players" extra="true">
			<visible>{game:players} &gt; 0 ? 1 : 0</visible>
			<text>{game:players} ${players}</text>
			<color>FFFFFF</color>
			<origin>0.5 0.5</origin>
			<pos>0.235 0.7185</pos>
			<size>0.075 0.035</size>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<text name="binds_noplayers" extra="true">
			<visible>{game:players} &gt; 0 ? 0 : 1</visible>
			<text>${noplayerdata}</text>
			<color>FFFFFF</color>
			<origin>0.5 0.5</origin>
			<pos>0.235 0.7185</pos>
			<size>0.075 0.035</size>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<alignment>center</alignment>
			<fontSize>0.020</fontSize>
			<zIndex>305</zIndex>
		</text>
		<image name="notation" extra="true">
			<origin>0.5 0.5</origin>
			<pos>0.31 0.72</pos>
			<maxSize>0.085 0.085</maxSize>
			<zIndex>305</zIndex>
			<linearSmooth>true</linearSmooth>
			<horizontalAlignment>left</horizontalAlignment>
			<path>${themePath}/_viewgame/fanartstar/Bvierge.png</path>
		</image>
		<!-- <image name="star" extra="true"> -->
		<!-- <origin>0.5 0.5</origin> -->
		<!-- <pos>0.31 0.72</pos> -->
		<!-- <maxSize>0.085 0.085</maxSize> -->
		<!-- <zIndex>305</zIndex> -->
		<!-- <linearSmooth>true</linearSmooth> -->
		<!-- <horizontalAlignment>left</horizontalAlignment> -->
		<!-- <unfilledPath>:/star_filled.svg</unfilledPath> -->
		<!-- </image> -->
		<text name="binds_rating" extra="true">
			<visible>{game:rating} &gt; 0 ? 1 : 0</visible>
			<text>{game:stars}</text>
			<pos>0.283 0.704</pos>
			<fontSize>0.020</fontSize>
			<size>0.00 0.00</size>
			<zIndex>305</zIndex>
			<color>FFFFFF</color>
		</text>
		<text name="binds_norating" extra="true">
			<visible>{game:rating} &gt; 0 ? 0 : 1</visible>
			<text>${noratings}</text>
			<origin>0.5 0.5</origin>
			<pos>0.309 0.7185</pos>
			<fontSize>0.020</fontSize>
			<alignment>center</alignment>
			<size>0.075 0.035</size>
			<fontPath>${themePath}/_viewgame/fanartstar/impact.ttf</fontPath>
			<zIndex>305</zIndex>
			<color>FFFFFF</color>
		</text>
		<!-- Gestion des disparitions pendant le scrolling -->
		<control name="systblack,dateblack,playblack,notation">
			<zIndex>205</zIndex>
			<opacity>0.7</opacity>
		</control>
		<control name="binds_players,binds_noplayers,binds_releasedate,binds_noreleasedate,binds_playcount,binds_noplaycount,developer,binds_rating,binds_norating,md_description">
			<color>FFFFFF</color>
			<opacity>0.5</opacity>
		</control>
	</customView>
</theme>
