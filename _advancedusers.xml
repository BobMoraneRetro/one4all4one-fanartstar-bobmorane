	</subset>
	<subset name="mediastar" displayName="${mediastar}" appliesTo="${vide}">
		<include name="desactive" displayName=" "/>
	</subset>
	<subset name="gridcolor" displayName="${fanarstargridcolor}" appliesTo="presetstar169,presetstar43">
		<include name="default" displayName="Par défaut"/>
		<include name="griscl" displayName="Gris Sympa"/>
		<include name="skyblue" displayName="Bleu Ciel"/>
		<include name="purple" displayName="Violet"/>
