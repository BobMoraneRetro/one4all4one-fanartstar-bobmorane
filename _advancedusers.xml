		<subset name="customfanart" displayName="${customfanart}" appliesTo="presetstarbox">
		<include name="fanartgamelist" displayName="${fanartgamelist}"/>
		<include name="fanartcustom" displayName="${fanartcustom}"/>
	</subset>
	<subset name="starboxang" displayName="${starboxang}" appliesTo="presetstarbox">
		<include name="active" displayName="${active}"/>
		<include name="deactive" displayName="${deactive}"/>
	</subset>
	<subset name="starthumb" displayName="${starthumb}" appliesTo="presetstarbox">
		<include name="starbox2d" displayName="${starbox2d}">./_viewgameoption/gl-fanartstar/2dbox.xml</include>
		<include name="starbox3d" displayName="${starbox3d}">./_viewgameoption/gl-fanartstar/3dbox.xml</include>
	</subset>
