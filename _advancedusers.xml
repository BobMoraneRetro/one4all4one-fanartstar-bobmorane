		<include name="yellow" displayName="Jaune"/>
		<include name="pink" displayName="Rose"/>
		<include name="grey" displayName="Gris foncé"/>
		<include name="lightgrey" displayName="Gris clair"/>
		<include name="navy" displayName="Bleu alt"/>
		<include name="violet" displayName="violet"/>
		<include name="candy" displayName="candy"/>
		<include name="redalt" displayName="Rouge Alt"/>
		<include name="yellowalt" displayName="Jaune Alt"/>
		<include name="greenalt" displayName="Vert Alt"/>
