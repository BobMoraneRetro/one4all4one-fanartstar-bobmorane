<?xml version="1.0" encoding="UTF-8"?>
<theme>
	<formatVersion>7</formatVersion>
	<subset name="systannonce" displayName="${systannounce}" appliesTo="coinops">
		<include name="classiquesyst" displayName="${classiquesyst}">${themePath}/_viewgame/commune/systeme/nomsystem.xml</include>
		<include name="desactive" displayName="${desactive}"/>
	</subset>
	<subset name="infosmax" displayName="${infosmax}" appliesTo="coinops">
		<include name="non" displayName="${no}"/>
		<include name="oui" displayName="${yes}">${themePath}/_viewgameoption/gl-common/infosmax.xml</include>
	</subset>
	<subset name="hiscore" displayName="${hiscore}" appliesTo="coinops">
		<include name="rankcadre" displayName="${rankcadre}">./_viewgameoption/gl-common/hi-score/hiscorecadre.xml</include>
		<include name="rankupcenter" displayName="${rankupcenter}">./_viewgameoption/gl-common/hi-score/hiscorecentre.xml</include>
		<include name="inactif" displayName="${inactif}"/>
	</subset>
	<subset name="hiscoretype" ifSubset="hiscore:rankupcenter|rankcadre" displayName="${hiscoretype}" appliesTo="coinops">
		<include name="top3" displayName="${top3}"/>
		<include name="top5" displayName="${top5}"/>
		<include name="allrank" displayName="${allrank}"/>
	</subset>
	<subset name="gwheelbackgr" displayName="${gwheelbackgr}" appliesTo="coinops">
		<include name="sisaccades" displayName="${sisaccades}"/>
