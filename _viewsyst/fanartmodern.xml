<theme>
	<formatVersion>7</formatVersion>
	<variables>
		<firstsystfanart>firstfile("${themePath}/_systemmedia/_fanartsysteme/fresh/" + {system:theme} + ".jpg","${themePath}/_systemmedia/_fanartsysteme/custom/" + {system:theme} + ".jpg")</firstsystfanart>
		<firstsystemimage>firstfile("${themePath}/_systemmedia/_fanartsysteme/custom/" + {system:theme} + ".jpg","${themePath}/_systemmedia/_fanartsysteme/fresh/" + {system:theme} + ".jpg")</firstsystemimage>
		<firstsystemhardaware>firstfile("${themePath}/_systemmedia/hardware/" + {system:theme} + ".png","${themePath}/_systemmedia/charsystem/" + {system:theme} + ".png")</firstsystemhardaware>
	</variables>
	<include>${themePath}/_systemmedia/description/${system.theme}.xml</include>
	<view name="system">
		<imagegrid name="imagegrid">
			<pos>0 0.4</pos>
			<size>1 0.51</size>
			<scrollDirection>horizontal</scrollDirection>
			<autoLayout>6 1</autoLayout>
			<padding>0.03 0.1 0.03 0.12</padding>
			<margin>0.035 0.1</margin>
			<!-- <margin>0.001 0.06</margin> -->
			<scrollbarColor>808080FF</scrollbarColor>
			<scrollbarCorner>0</scrollbarCorner>
			<scrollbarSize>0.003</scrollbarSize>
			<autoLayoutSelectedZoom>1.2</autoLayoutSelectedZoom>
			<!-- <cellProportion>0.1</cellProportion> -->
			<centerSelection>false</centerSelection>
			<zIndex>50</zIndex>
			<itemTemplate>
				<container>
					<image name="thumb">
						<roundCorners>0.03</roundCorners>
						<pos>0.5 0.505</pos>
						<origin>0.5</origin>
						<minSize>1.15 1.39</minSize>
						<path>${firstsystemimage}</path>
						<!-- <saturation>0</saturation> -->
						<zIndex>5</zIndex>
						<shader>
							<path>:/shaders/blur.glsl</path>
							<pos>0 0</pos>
							<blur>10</blur>
							<zIndex>1</zIndex>
							<blurColor>000000</blurColor>
						</shader>
						<linearSmooth>true</linearSmooth>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="color" to="FFFFFFFF" begin="" duration="150" mode="Linear"/>
							<animation property="scale" to="1.2" begin="" duration="350" mode="easeOut"/>
							<animation property="saturation" to="1" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="color" to="858585ff" begin="" duration="250" mode="Linear"/>
							<animation property="scale" to="1" begin="" duration="350" mode="easeOut"/>
							<animation property="saturation" to="0" begin="" duration="150" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="color" to="858585ff" begin="" duration="" mode="Linear"/>
							<animation property="scale" to="1" begin="" duration="350" mode="easeOut"/>
							<animation property="saturation" to="0" begin="" duration="150" mode="Linear"/>
							<!-- <animation property="y" to="0.6" begin="" duration="" mode="Linear"/> -->
						</storyboard>
					</image>
					<image name="vignette">
						<roundCorners>0.03</roundCorners>
						<path>${themePath}/_viewsyst/fanartstar/vignette3.png</path>
						<pos>0.5 0.505</pos>
						<origin>0.5</origin>
						<minSize>1.15 1.39</minSize>
						<zIndex>5</zIndex>
						<storyboard event="open">
							<animation property="rotation" to="-3" begin="" duration="350" mode="Linear"/>
						</storyboard>
						<storyboard event="activate">
							<animation property="scale" to="1.2" begin="" duration="350" mode="easeOut"/>
							<animation property="color" from="858585ff" to="FFFFFFFF" begin="10" duration="150" mode="Linear" autoReverse="true" repeat="forever"/>
						</storyboard>
						<storyboard event="deactivate">
							<animation property="scale" to="1" begin="" duration="350" mode="easeOut"/>
							<animation property="opacity" to="1" begin="" duration="" mode="Linear"/>
						</storyboard>
						<storyboard event="scroll">
							<animation property="scale" to="1" begin="" duration="350" mode="easeOut"/>
							<animation property="opacity" to="1" begin="" duration="" mode="Linear"/>
						</storyboard>
					</image>
					<image name="cadre">
						<path>${themePath}/_viewsyst/fanartstar/cadre.png</path>
						<linearSmooth>true</linearSmooth>
						<origin>0.5</origin>
						<pos>0.51 0.5</pos>
						<size>1.18 1.42</size>
						<zIndex>7</zIndex>
						<color>FFFFFFFF</color>
						<storyboard event="open">
